<table >  <tbody>    <tr>      <td colspan="4" rowspan="1">	  <h3>ServerRoom Environment</h3>	  </td>    </tr>      <td >		Room Name</td>      <td >		<input class="ui-widget ui-state-default ui-corner-all ui-textinput" size="30" name="room_name">	  </td>      <td >		Site Induction / Access Logs</td>      <td>      <select name="induction">      <option></option>      <option>Visitor Book</option>      <option>Log Book</option>      <option>Other</option>      </select>      </td>    </tr>	    <tr>      <td>No of Racks in Room</td>      <td ><input  id="rack_no_spinner" size="5" name="no_racks">	  Room Level		<input id="roomlevel_spinner" size="5" value="G" name="room_level">	  </td>    </tr>    <tr>      <td >Room Type </td>      <td >      <select name="Room_Type">      <option selected="selected">Secured</option>      <option value="Unsecured">Unsecured /shared</option>      <option>other</option>      </select>      </td>	        <td>Room Access </td>      <td>      <select name="room_access">      <option selected="selected"></option>      <option>key lock</option>      <option>swipe</option>      <option>other - add comment</option>      </select>      </td>	  </tr>	  <tr  bgcolor="#FFFFFF">      <td>Room Dimensions</td>	  <td>	 <p>    <label for="room_height">Height</label>    <input type="text" id="room_height" style="border: 0; color: #f6931f; font-weight: bold;" />	<div id="slider_room_height"></div>    <label for="room_width">Width  </label>    <input type="text" id="room_width" style="border: 0; color: #f6931f; font-weight: bold;" />	</p> <div id="slider_room_width"></div><label for="room_depth">Depth  </label>    <input type="text" id="room_depth" style="border: 0; color: #f6931f; font-weight: bold;" />	</p> <div id="slider_room_depth"></div>	</td>      <td>Door Dimensions</td>	  <td>	 <p>    <label for="height">Height</label>    <input type="text" id="height" style="border: 0; color: #f6931f; font-weight: bold;" />	<div id="slider_door_height"></div>    <label for="width">Width  </label>    <input type="text" id="width" style="border: 0; color: #f6931f; font-weight: bold;" />	</p> <div id="slider_door_width"></div>	</td>	</tr>    <tr>      <td>Floor Type</td>      <td >      <select name="floor_type">      <option selected="selected"></option>      <option>vinyl</option>      <option>carpet</option>      <option>concrete</option>      <option>raised</option>      <option>other - add comment</option>      </select>      </td>       <td >Ceiling Type</td>      <td >      <select name="ceiling_type">      <option></option>      <option>solid</option>      <option>false (suspended)</option>      <option>other -add comment</option>      </select>      </td>    </tr>    <tr>      <td>	  10 Amp Power Outlets</td>	  <td>	  Total	  <input id="10amp_spinner" name="10amp_total" size="5" />	  Used	  <input id="10amp_used_spinner" name="10amp_used" size="5" />	  </td>	        <td> 15 Amp Power Outlets</td>      <td>Total	  <input id="15amp_spinner" name="15amp_total" size="5" />	  Used	  <input id="15amp_used_spinner" name="15amp_used" size="5" />	  </td>	      </tr>	  <tr>	  <td>	  </td>      <td valign="top">	  <input type="checkbox" id="room_fire_check" class="room_fire_check">	  <label for="room_fire_check" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" role="button" aria-disabled="false"><p>Fire Detection </p></label>	    <div class="room_fire_div">        <p>Fire Detection/Suppression Type</p>       <input size="50" name="fire_detection">    <p>Fire Detection/Suppression Brand / Model etc.</p>    <input size="50" name="fire_model">           </div>	  </td>      <td valign="top">	  	  <input type="checkbox" id="room_cooling_check" class="room_cooling_check">	  <label for="room_cooling_check" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" role="button" aria-disabled="false"><p>	  	  Room Cooling</p></label>		    <div class="room_cooling_div">		<p>		Type		</p>		<p>		<select name="cooling_Type">      <option selected="selected" value="split">split system</option>      <option value="building">building</option>	  <option value="dedicated">dedicated</option>      <option>other</option>      </select>		</p>      <p>Room Cooling (Make and Model)</p>      <input size="50" name="aircon_type">      <p>Air Conditioning Capacity (KW / HP)</p>		<input size="50" name="aircon_capacity">		    </div>	  </td>    </tr>     <tr>      <td>      <p>Other General Comments</p>      </td>      <td colspan="3" rowspan="1">	  <textarea cols="100" rows="3" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="general_comments">	  </textarea>	  </td>    </tr>  </tbody></table>