<table>  <tbody>    <tr>      <td colspan="4" rowspan="1" ><h3>Rack Details</h3></td>    </tr>    <tr>      <td >Rack Identification</td>      <td ><input size="30" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_rack_name"></td>      <td >Location </td>      <td ><input size="30" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_location"></td>    </tr>    <tr>      <td >Rack Model </td>      <td >      <input size="30" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_model"></td>      <td >Coupling to other racks</td>      <td >      <select name="rack_coupling">      <option>no</option>      <option>yes</option>      </select>      </td>    </tr>	    <tr>      <td>Rack Dimensions</td>		<td>			<p>RU     <input class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_height_ru"></p>			<p>Width  <input class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_width"></p>			<p>Depth  <input class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_depth"></p>      </td>      <td>Clearance from Walls / other racks with doors closed</td>      <td >      <table >        <tbody>          <tr>            <td>Left</td>            <td><input size="5" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_clr_left"></td>          </tr>          <tr>            <td>Right</td>            <td><input size="5" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_clr_left"></td>          </tr>          <tr>            <td>Front</td>            <td><input size="5" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_clr_left"></td>          </tr>          <tr>            <td>Back</td>            <td><input size="5" class="ui-widget ui-state-default ui-corner-all ui-textinput" name="rack_clr_left"></td>          </tr>        </tbody>      </table>      <br>      </td>    </tr>	<tr>	<td>Percentage occupied</td>		<td>    <input type="text" size="2" class="ui-widget ui-state-default ui-corner-all ui-textinput" id="rack_percentage" value="50"  />%		<div id="slider_rack_percentage" style="width: 100px;" ></div>	</td>	</tr>    <tr>      <td>Front Door details</td>	                   <td>				<div id="format">				<input id="check1" name="rack_fd_key_lock" type="checkbox"><label for="check1">Key Lock</label>				<input id="check2" name="rack_fd_mesh" type="checkbox"><label for="check2">Mesh</label>				<input id="check3" name="rack_fd_glass" type="checkbox"><label for="check3">Glass</label>				<input id="check4" name="rack_fd_metal" type="checkbox"><label for="check4">Metal</label>				</div>			</td>      <td >Back Door details</td>                 <td>				<div id="format">				<input id="check5" name="rack_bd_key_lock" type="checkbox"><label for="check5">Key Lock</label>				<input id="check6" name="rack_bd_mesh" type="checkbox"><label for="check6">Mesh</label>				<input id="check7" name="rack_bd_glass" type="checkbox"><label for="check7">Glass</label>				<input id="check8" name="rack_bd_metal" type="checkbox"><label for="check8">Metal</label>				</div>			</td>    </tr>    <tr>      <td>Top cover Details </td>      <td>      <input size="50" name="rack_top_cover"></td>      <td>Extraction Fans</td>      <td>      <input size="50" name="rack_extraction_fans"></td>    </tr>    <tr>      <td>Side Panel Details</td>      <td ><input size="50" name="rack_side_panel"></td>      <td ></td>      <td ></td>    </tr>    <tr>      <td>Rack Cable Management<br>      </td>      <td >      <select name="rack_cm">      <option>yes</option>      <option>no</option>      </select>      </td>      <td >Rack Power (UPS / Direct) with details</td>      <td ><input size="50" name="rack_power"></td>    </tr>    <tr>      <td>No of PDU’s with description (Vertical / Horizontal etc.,)</td>      <td><input size="50" name="rack_pdu"></td>      <td>PDU Details</td>      <td>      <table>        <tbody>          <tr>            <td>Outlet Type</td>            <td><input size="5" name="rack_pdu_outlet"></td>          </tr>          <tr>            <td>Number of Outlets</td>            <td><input size="5"  name="rack_pdu_number_outlets"></td>          </tr>          <tr>            <td>In use</td>            <td><input size="5"  name="rack_pdu_outlets_inuse"></td>          </tr>        </tbody>      </table>      </td>    </tr>    <tr>      <td>      <p>General Comments<o:p></o:p></p>      </td>      <td colspan="3" rowspan="1"><textarea class="ui-widget ui-state-default ui-corner-all ui-textinput" cols="100" rows="3" name="rack_general_comments"></textarea></td>    </tr>  </tbody></table>